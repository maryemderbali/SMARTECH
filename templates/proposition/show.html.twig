{% extends 'base.html.twig' %}

{% block title %}Proposition{% endblock %}

{% block body %}
    <h1>Proposition</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>NomPrestataire</th>
                <td>{{ proposition.nomPrestataire }}</td>
            </tr>
            <tr>
                <th>IdEntreprise</th>
                <td>{{ proposition.idEntreprise }}</td>
            </tr>
            <tr>
                <th>NumeroTelephone</th>
                <td>{{ proposition.numeroTelephone }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ proposition.mail }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ proposition.titre }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ proposition.montant }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ proposition.message }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_proposition_index') }}">back to list</a>

    <a href="{{ path('app_proposition_edit', {'nomPrestataire': proposition.nomPrestataire}) }}">edit</a>

    {{ include('proposition/_delete_form.html.twig') }}
{% endblock %}
